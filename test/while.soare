
loadimport "script/std.min.soare";

write ""
$times = 0;

while times < 5 + 1 do
  
  write "Please wait 5 seconds: [", times, "/", 5, "] ";

  $progressbar = 0;
  while progressbar < times + 1 do
    write "=";
    progressbar = progressbar + 1;
  end;

  sleep(1);
  times = times + 1;

  write "\r";

end;

write "\n\t===== SUCESS =====\n";
