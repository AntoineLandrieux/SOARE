
loadimport "script/std.soare"

let times = 0;

while (1)

  write("Please wait 5 seconds: ["; times; '/'; 5; "] ");

  let progress = 0;

  while (progress < times)
    write('=');
    progress = progress + 1;
  end;

  if (times == 5)
    break;
  end;

  sleep(1);
  times = times + 1;

  write('\r');

end;

write('\n');
